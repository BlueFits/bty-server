doctype html
html
    header
        title="visitors"
        link(rel="stylesheet" href="/stylesheets/style.css")
    body
        div(class="contentWrapper")
            h2="Menu"
            p.
                Logged in as #[strong #{admin.username}]
        div(class="contentWrapper menuStyles")
            p
                a(href="/admin/dashboard")="Home"
            p
                a(href="/admin/userList")="List of Users"
            p
                a(href="/admin/visitors")="Visitors"
            p
                a(href="/admin/logout")="Logout"
        div(class="contentWrapper")
            h2=user.email
            div
                p="Joined at: " + moment(user.createdAt).format("MMMM Do YYYY")
                -
                    const lastLogIn = !user.loggedAt[4] ? "Never Logged In" : moment(user.loggedAt[4]).format("dddd, MMMM Do YYYY, h:mm:ss a")
                p= "Last logged in: " + lastLogIn
            div
                p="Last 5 logs"
                ul  
                    each val in user.loggedAt
                        li=moment(val).format("dddd, MMMM Do YYYY, h:mm:ss a")    